{
    "name": "lavoaster/vault-env",
    "description": "Connects to a HashiCorp vault server and reads the given secret into environment variables",
    "license": "MIT",
    "authors": [
        {
            "name": "Adam Lavin",
            "email": "adam@lavoaster.co.uk"
        }
    ],
    "require": {
        "csharpru/vault-php": "^3.3",
        "guzzlehttp/guzzle": "^6.3",
        "csharpru/vault-php-guzzle6-transport": "^2.0",
        "monolog/monolog": "^1.23"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4",
        "symfony/var-dumper": "^3.3",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Lavoaster\\VaultEnv\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lavoaster\\VaultEnv\\Tests\\": "tests"
        }
    }
}
